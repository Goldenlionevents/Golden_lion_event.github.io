}
.solid-button-container1
{
	border:1px solid yellow;
	  display: flex;
  position: relative;
  margin:10px;
  padding:10px;


}
.solid-button {
  color: #FFFFFF;
  border: none;
  padding-top: 0.5em;
  padding-left: 2em;
  border-radius: 50px;
  padding-right: 2em;
  padding-bottom: 0.5em;
  background-color:#921636ff;
      box-shadow: 10px 10px 20px 0px black;
  
}
.contact
{
	font-size:20px;
    display:flex;
    justify-content:right;
	margin-right:300px;
	margin-top:-200px;
    padding-top: 150px;
	color: white;
	text-shadow:5px 5px 5px black;
}
.footer
{
